{
    "name": "@concordium/node-sdk",
    "version": "0.1.0",
    "description": "Helpers for interacting with the Concordium node",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Concordium/concordium-node-sdk-js"
    },
    "author": {
        "name": "Concordium Software",
        "email": "support@concordium.software",
        "url": "https://concordium.com"
    },
    "license": "Apache-2.0",
    "private": false,
    "devDependencies": {
        "grpc-tools": "^1.11.2",
        "grpc_tools_node_protoc_ts": "5.3.0"
    },
    "scripts": {
        "generate-js": "yarn run grpc_tools_node_protoc --js_out=import_style=commonjs,binary:src/grpc --grpc_out=grpc_js:src/grpc --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin -I ./deps/concordium-grpc-api deps/concordium-grpc-api/*.proto",
        "generate-ts": "yarn run grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=src/grpc -I ./deps/concordium-grpc-api deps/concordium-grpc-api/*.proto",
        "generate": "yarn generate-js && yarn generate-ts"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.3.4"
    }
}
