{
    "include": ["src/**/*"],
    "exclude": ["node_modules", "grpc/**/*"],
    "rerefences": [
        { "path": "../common"}
    ],
    "compilerOptions": {
        "strict": true,
        "lib": ["ES2020"],
        "module": "commonjs",
        "declaration": true,
        "target": "ES2020",
        "resolveJsonModule": true,
        "rootDir": "src",
        "outDir": "./lib/src"
    }
}
